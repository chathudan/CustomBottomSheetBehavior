<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="co.com.parsoniisolutions.custombottomsheetbehavior">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" /> <!-- Needed by Google Maps API -->
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <!-- API key for google maps -->
        <meta-data
                android:name="com.google.android.maps.v2.API_KEY"
                android:value="AIzaSyCXBlAxBBS7y1qtJLFVzwejz7VblxhkdWA" /> <!-- Your API key goes here -->

        <activity
            android:name=".sample.simple.MainActivitySimple"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <!-- android:name=".sample.simple.MainActivitySimple" -->
            <!-- android:name=".sample.withloading.MainActivityWithLoading" -->

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>